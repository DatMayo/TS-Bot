{
    "name": "ts_bot",
    "version": "0.0.1",
    "description": "",
    "scripts": {
        "build": "rollup -c && npm run obfuscate",
        "build:watch": "rollup -cw",
        "debug": "nodemon ./build/bundle.plain.js",
        "lint": "eslint .",
        "obfuscate": "javascript-obfuscator ./build/bundle.plain.js --output ./build/bundle.js --config obfuscator.json"
    },
    "repository": {
        "type": "git",
        "url": "git@git.reloaded-life.de:Reloaded-Life/ts_bot.git"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^17.1.0",
        "@rollup/plugin-json": "^4.1.0",
        "@rollup/plugin-node-resolve": "^11.1.1",
        "@rollup/plugin-typescript": "^8.1.1",
        "@types/node": "^14.14.25",
        "@typescript-eslint/eslint-plugin": "^4.14.2",
        "@typescript-eslint/parser": "^4.14.2",
        "eslint": "^7.19.0",
        "eslint-config-prettier": "^7.2.0",
        "eslint-plugin-prettier": "^3.3.1",
        "javascript-obfuscator": "^2.10.3",
        "nodemon": "^2.0.7",
        "prettier": "^2.2.1",
        "typescript": "^4.1.3"
    },
    "dependencies": {
        "dotenv": "^8.2.0",
        "ts3-nodejs-library": "^3.2.0"
    }
}
